<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jigneshpanchal.ltepeakdlthroughputestimator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ltepeaktp"
        android:label="@string/app_name"
        android:configChanges="keyboardHidden|orientation|screenSize|screenLayout"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BandConfigurationActivity"
            android:label="@string/title_activity_band_configuration"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.jigneshpanchal.ltepeakdlthroughputestimator.MainActivity" />
        </activity>
    </application>

</manifest>